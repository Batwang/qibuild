"""QiBuild command line tool.
"""

import argparse
import qitools.argparsecommand

def main():
    parser = argparse.ArgumentParser()
    modules = qitools.argparsecommand.action_modules_from_package("qibuild.actions")
    qitools.argparsecommand.root_command_main("qibuild", parser, modules)

if __name__ == "__main__":
    main()
